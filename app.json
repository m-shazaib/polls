{
  "expo": {
    "name": "polls",
    "slug": "polls",
    "scheme": "polls-to-poll",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.avif",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.avif",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.avif",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mshazaib.polls"
    },
    "web": {
      "favicon": "./assets/icon.avif"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4775600c-449e-477a-8127-7c48b555c2d0"
      }
    }
  }
}
